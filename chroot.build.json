        },
        "build-lucid64": {
            "release": "lucid",
            "packages": ["boost-build"]
        },
        "build-precise32": {
            "release": "precise",
            "conf": {
                "personality": "linux32"
            }
        },
        "build-precise64": {
            "release": "precise"
        },
        "build-quantal32": {
            "release": "quantal",
            "conf": {
                "personality": "linux32"
            }
        },
        "build-quantal64": {
            "release": "quantal"
        },
        "build-raring32": {
            "release": "raring",
            "conf": {
                "personality": "linux32"
            }
        },
        "build-raring64": {
            "release": "raring"
        }
