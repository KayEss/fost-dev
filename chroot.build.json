{
    "root": "/mnt/files4/chroot",
    "source": "http://th.archive.ubuntu.com/ubuntu/",
    "http-proxy": "http://angelo:3142/",
    "base-packages": [
        "bash-completion", "lsb-release", "nano", "ssh-client",
        "g++", "libbz2-dev", "libssl-dev", "python-dev", "uuid-dev",
        "libboost-all-dev", "subversion", "git-core"
    ],
    "defaults": {
        "variant": "buildd",
        "sources": {
            "universe": {}
        },
        "conf": {
            "root-users": ["kirit"],
            "users": ["kirit"]
        }
    },
    "schroot": {
        "build-lucid32": {
            "release": "lucid",
